{
  "dockerComposeFile": "docker-compose.yml",
  "mounts": [
    "source=${localEnv:HOME}/.gnupg,target=/home/dev/.gnupg,consistency=cached"
  ],
  "remoteUser": "python",
  "service": "ironbank-pipeline",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "settings": {
        "python.defaultInterpreterPath": "/opt/poetry/bin/python"
      },
      "extensions": ["ms-python.python"]
    }
  }
}
