  "regexManagers": [
    {
      "fileMatch": ["^rhel-oscap-version.json$"],
      "matchStrings": ["\"version\":\\s+\"(?<currentValue>.*?)\""],
      "depNameTemplate": "ComplianceAsCode/content",
      "datasourceTemplate": "github-releases"
    }
  ]
}
